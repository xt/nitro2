    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coverageReporters": [
			"text",
			"html"
		]

  },
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
